{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
      {
          "username": "diabagatekelly",
          "password": "kellya22",
          "first_name": "Kelly",
          "last_name": "Diabagate",
          "email": "diabagatekelly@yahoo.fr",
          "business": false,
          "banking": false,
          "islamic": false
      }
  ],
  "required": [
      "username",
      "password",
      "first_name",
      "last_name",
      "email",
      "business",
      "banking",
      "islamic"
  ],
  "properties": {
      "username": {
          "$id": "#/properties/username",
          "type": "string",
          "title": "The username schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "diabagatekelly"
          ]
      },
      "password": {
          "$id": "#/properties/password",
          "type": "string",
          "title": "The password schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "kellya22"
          ]
      },
      "first_name": {
          "$id": "#/properties/first_name",
          "type": "string",
          "title": "The first_name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Kelly"
          ]
      },
      "last_name": {
          "$id": "#/properties/last_name",
          "type": "string",
          "title": "The last_name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Diabagate"
          ]
      },
      "email": {
          "$id": "#/properties/email",
          "type": "string",
          "title": "The email schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "diabagatekelly@yahoo.fr"
          ]
      },
      "business": {
          "$id": "#/properties/business",
          "type": "boolean",
          "title": "The business schema",
          "description": "An explanation about the purpose of this instance.",
          "default": false,
          "examples": [
              false
          ]
      },
      "banking": {
          "$id": "#/properties/banking",
          "type": "boolean",
          "title": "The banking schema",
          "description": "An explanation about the purpose of this instance.",
          "default": false,
          "examples": [
              false
          ]
      },
      "islamic": {
          "$id": "#/properties/islamic",
          "type": "boolean",
          "title": "The islamic schema",
          "description": "An explanation about the purpose of this instance.",
          "default": false,
          "examples": [
              false
          ]
      }
  },
  "additionalProperties": true
}